<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="s" class="com.model.Student">
        <property name="rollno" value="1"></property>
        <property name="name" value="sb"></property>
        <property name="add" ref="add"></property>
        
    </bean>
    <bean id="add" class="com.model.Address">
    <property name="city" value="pune"></property>
    <property name="area" value="Karve"></property>
    </bean>
    

</beans>