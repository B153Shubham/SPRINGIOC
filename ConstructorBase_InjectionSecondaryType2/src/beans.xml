<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="s" class="com.model.Student">
        <constructor-arg name="rollno" value="1"></constructor-arg>
        <constructor-arg name="name" value="sb"></constructor-arg>
        <constructor-arg name="add" ref="add"></constructor-arg>
        
    </bean>
    <bean id="add" class="com.model.Address">
    <constructor-arg name="city" value="pune"></constructor-arg>
    <constructor-arg name="area" value="Karve"></constructor-arg>
    </bean>
    

</beans>